{
  "schema": {
    "$ref": "#/$defs/file-commit"
  },
  "$defs": {
    "IApiReposContents.PutBody": {
      "type": "object",
      "properties": {
        "message": {
          "description": "The commit message.",
          "type": "string"
        },
        "content": {
          "description": "The new file content, using Base64 encoding.",
          "type": "string"
        },
        "sha": {
          "description": "**Required if you are updating a file**. The blob SHA of the file being replaced.",
          "type": "string"
        },
        "branch": {
          "description": "The branch name. Default: the repositoryâ€™s default branch.",
          "type": "string"
        },
        "committer": {
          "description": "The person that committed the file. Default: the authenticated user.",
          "type": "object",
          "properties": {
            "name": {
              "description": "The name of the author or committer of the commit. You'll receive a `422` status code if `name` is omitted.",
              "type": "string"
            },
            "email": {
              "description": "The email of the author or committer of the commit. You'll receive a `422` status code if `email` is omitted.",
              "type": "string"
            },
            "date": {
              "example": "\"2013-01-05T13:13:22+05:00\"",
              "type": "string"
            }
          },
          "required": [
            "name",
            "email"
          ]
        },
        "author": {
          "description": "The author of the file. Default: The `committer` or the authenticated user if you omit `committer`.",
          "type": "object",
          "properties": {
            "name": {
              "description": "The name of the author or committer of the commit. You'll receive a `422` status code if `name` is omitted.",
              "type": "string"
            },
            "email": {
              "description": "The email of the author or committer of the commit. You'll receive a `422` status code if `email` is omitted.",
              "type": "string"
            },
            "date": {
              "example": "\"2013-01-15T17:13:22+05:00\"",
              "type": "string"
            }
          },
          "required": [
            "name",
            "email"
          ]
        }
      },
      "required": [
        "message",
        "content"
      ]
    },
    "file-commit": {
      "title": "File Commit",
      "description": "File Commit",
      "type": "object",
      "required": [
        "content",
        "commit"
      ],
      "properties": {
        "content": {
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "path": {
                  "type": "string"
                },
                "sha": {
                  "type": "string"
                },
                "size": {
                  "type": "integer"
                },
                "url": {
                  "type": "string"
                },
                "html_url": {
                  "type": "string"
                },
                "git_url": {
                  "type": "string"
                },
                "download_url": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                },
                "_links": {
                  "type": "object",
                  "properties": {
                    "self": {
                      "type": "string"
                    },
                    "git": {
                      "type": "string"
                    },
                    "html": {
                      "type": "string"
                    }
                  },
                  "required": []
                }
              },
              "required": []
            },
            {
              "type": "null"
            }
          ]
        },
        "commit": {
          "type": "object",
          "properties": {
            "sha": {
              "type": "string"
            },
            "node_id": {
              "type": "string"
            },
            "url": {
              "type": "string"
            },
            "html_url": {
              "type": "string"
            },
            "author": {
              "type": "object",
              "properties": {
                "date": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                }
              },
              "required": []
            },
            "committer": {
              "type": "object",
              "properties": {
                "date": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                }
              },
              "required": []
            },
            "message": {
              "type": "string"
            },
            "tree": {
              "type": "object",
              "properties": {
                "url": {
                  "type": "string"
                },
                "sha": {
                  "type": "string"
                }
              },
              "required": []
            },
            "parents": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "url": {
                    "type": "string"
                  },
                  "html_url": {
                    "type": "string"
                  },
                  "sha": {
                    "type": "string"
                  }
                },
                "required": []
              }
            },
            "verification": {
              "type": "object",
              "properties": {
                "verified": {
                  "type": "boolean"
                },
                "reason": {
                  "type": "string"
                },
                "signature": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "payload": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "verified_at": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                }
              },
              "required": []
            }
          },
          "required": []
        }
      }
    }
  }
}