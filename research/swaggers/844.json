        "_links": {
          "type": "object",
          "properties": {
            "git": {
              "oneOf": [
                {
                  "type": "string",
                  "format": "uri"
                },
                {
                  "type": "null"
                }
              ]
            },
            "html": {
              "oneOf": [
                {
                  "type": "string",
                  "format": "uri"
                },
                {
                  "type": "null"
                }
              ]
            },
            "self": {
              "type": "string",
              "format": "uri"
            }
          },
          "required": [
            "git",
            "html",
            "self"
          ]
        },
        "target": {
          "example": "\"actual/actual.md\"",
          "type": "string"
        },
        "submodule_git_url": {
          "example": "\"git://example.com/defunkt/dotjs.git\"",
          "type": "string"
        }
      },
      "required": [
        "_links",
        "git_url",
        "html_url",
        "download_url",
        "name",
        "path",
        "sha",
        "size",
        "type",
        "url",
        "content",
        "encoding"
