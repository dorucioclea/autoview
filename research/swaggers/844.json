{
  "schema": {
    "$ref": "#/$defs/content-file"
  },
  "$defs": {
    "IApiReposReadme.GetQuery": {
      "type": "object",
      "properties": {
        "ref": {
          "description": "The name of the commit/branch/tag. Default: the repositoryâ€™s default branch.",
          "type": "string"
        }
      },
      "required": []
    },
    "content-file": {
      "title": "Content File",
      "description": "Content File",
      "type": "object",
      "properties": {
        "type": {
          "const": "file"
        },
        "encoding": {
          "type": "string"
        },
        "size": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "content": {
          "type": "string"
        },
        "sha": {
          "type": "string"
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "git_url": {
          "oneOf": [
            {
              "type": "string",
              "format": "uri"
            },
            {
              "type": "null"
            }
          ]
        },
        "html_url": {
          "oneOf": [
            {
              "type": "string",
              "format": "uri"
            },
            {
              "type": "null"
            }
          ]
        },
        "download_url": {
          "oneOf": [
            {
              "type": "string",
              "format": "uri"
            },
            {
              "type": "null"
            }
          ]
